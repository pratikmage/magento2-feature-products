<?xml version="1.0" encoding="UTF-8"?>


<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="formkey"/>
    <head>
        <title>Feature Products</title>
    </head>
    <body>        
        <referenceBlock name="page.title">
            <action method="setPageTitle">
                <argument name="pageTitle" xsi:type="string">Feature Products</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Brainvire\ProductGrid\Block\Adminhtml\Featureproduct" name="admin.block.featureproduct.grid.container"/>
        </referenceContainer>       
    </body>
</page>